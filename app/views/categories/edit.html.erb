
<section id="title-bar">
     <div class="container">
       <div class="row">
         <div class="col-md-12">
           <h1> Edit Form </h1>
         </div>
       </div>
     </div>
   </section>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@category) do |f| %>
    
    <%= f.label :name %><br/>
    <%= f.text_field :name %><br/>
    
    <%= f.label :description %><br/><br/>
    <%= f.text_field :description %> <br/>
    
    <%= f.submit class="btn btn-default" %> 
    
   <%end%>
  </div>
</div>
